#include<stdio.h>
int main()
{
   
   int num; 
   printf("Enter the Number: ");
   scanf("%d", &num);
   check_prime(num);
}


int check_prime(int p)
{
    int i, count =0;
    for(i=2; i<p; i++)
    {
        if(p%i==0)
        {
            count++;
            break;
        }
        
    }
     if(count == 0)
   {
       printf("the number is Prime");
   }
   else
   {
       printf("The number is not Prime");
   }
   
}
